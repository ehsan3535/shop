@using Shop.Models
@model List<UsersDto>

<form method="post" asp-action="ListUser">
    @foreach (var item in Model)
    {
        <div>

            <h1>@item.Name</h1>
            <h1>@item.PhoneNumber</h1>
            <h1>@item.Email</h1>
            <h1>@item.NationalCode</h1>
            <h1>@item.Username</h1>
            <h1>@item.Id</h1>
            <h2>-----------------------</h2>
            <br>
        </div>

    }


</form>